{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "flowtype"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-useless-constructor": "off"
  },
  "env": {
    "node": true,
    "jest": true
  }
}
